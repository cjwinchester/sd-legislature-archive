, {"meeting_datetime": "2007-02-21T00:00:00-06:00", "committee": "SJU", "url": "https://sdpb.sd.gov/sdpbpodcast/2007/sju29.mp3", "start_seconds": 652.0}, {"meeting_datetime": "2007-02-21T00:00:00-06:00", "committee": "SJU", "url": "https://sdpb.sd.gov/sdpbpodcast/2007/sju29.mp3", "start_seconds": 652.0}, {"meeting_datetime": "2007-02-21T00:00:00-06:00", "committee": "SJU", "url": "https://sdpb.sd.gov/sdpbpodcast/2007/sju29.mp3", "start_seconds": 652.0}], "bill_versions": [{"bill_id": 21008, "bill_version_id": 110244, "bill_version": "Introduced", "bill_version_date": "2007-01-16T08:42:51-06:00", "bill_text": "FOR AN ACT ENTITLED, An Act to permit magistrate courts to administer adult probationary drug court programs under certain circumstances. BE IT ENACTED BY THE LEGISLATURE OF THE STATE OF SOUTH DAKOTA: Section 1. That chapter 16-12B be amended by adding thereto a NEW SECTION to read as follows: Any magistrate court with a magistrate judge presiding has authority, upon assignment of the presiding judge of the circuit, to act in lieu of a circuit judge having jurisdiction heretofore granted to circuit courts to administer and preside over an adult probationary drug court program. 250 copies of this document were printed by the South Dakota Legislative Research Council at a cost of $.038 per page. . Insertions into existing statutes are indicated by underscores. Deletions from existing statutes are indicated by overstrikes."}, {"bill_id": 21008, "bill_version_id": 112561, "bill_version": "Senate Judiciary Engrossed", "bill_version_date": "2007-02-21T12:47:03-06:00", "bill_text": "FOR AN ACT ENTITLED, An Act to permit magistrate courts to administer adult probationary drug court programs under certain circumstances. BE IT ENACTED BY THE LEGISLATURE OF THE STATE OF SOUTH DAKOTA: Section 1. That chapter 16-12B be amended by adding thereto a NEW SECTION to read as follows: Any magistrate court with a magistrate judge presiding has authority, upon assignment of the presiding judge of the circuit, to act in lieu of a circuit judge having jurisdiction to administer and preside over an adult probationary drug court program."}, {"bill_id": 21008, "bill_version_id": 112982, "bill_version": "Enrolled", "bill_version_date": "2007-02-28T07:35:41-06:00", "bill_text": ""}], "amendments": [], "fiscal_notes": [], "action_log": [{"bill_id": 21008, "action_date": "2007-01-16T14:00:00-06:00", "document_id": 110292, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/110292.pdf", "status_text": "First read in House and referred to", "journal_page": 80, "committee_id_action": 923, "committee_id_assigned": 921, "result": "N", "vote": {}}, {"bill_id": 21008, "action_date": "2007-01-26T07:45:00-06:00", "document_id": 110927, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/110927.pdf", "status_text": "Scheduled for Committee hearing on this date", "journal_page": null, "committee_id_action": 921, "committee_id_assigned": null, "result": "N", "vote": {}}, {"bill_id": 21008, "action_date": "2007-01-26T07:45:00-06:00", "document_id": 110927, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/110927.pdf", "status_text": "Do Pass,", "journal_page": 221, "committee_id_action": 921, "committee_id_assigned": null, "result": "P", "vote": {"vote_id": 66347, "president_vote": null, "Yea": [3579, 3592, 3594, 3598, 3599, 3619, 3632, 3638, 3651, 3672, 3673, 3582], "Excused": [3676]}}, {"bill_id": 21008, "action_date": "2007-01-29T14:00:00-06:00", "document_id": 111076, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/111076.pdf", "status_text": "Deferred to another day", "journal_page": 251, "committee_id_action": 923, "committee_id_assigned": null, "result": "P", "vote": {}}, {"bill_id": 21008, "action_date": "2007-01-30T14:00:00-06:00", "document_id": 111185, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/111185.pdf", "status_text": "Do Pass,", "journal_page": 268, "committee_id_action": 923, "co