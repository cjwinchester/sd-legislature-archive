tend."}, {"bill_id": 5854, "bill_version_id": 31522, "bill_version": "Enrolled", "bill_version_date": "2014-03-05T09:50:17.473-06:00", "bill_text": ""}], "amendments": [], "fiscal_notes": [], "action_log": [{"bill_id": 5854, "action_date": "2014-01-29T14:00:00-06:00", "document_id": 29326, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/29326.pdf", "status_text": "First read in House and referred to", "journal_page": 147, "committee_id_action": 231, "committee_id_assigned": 217, "result": "N", "vote": {}}, {"bill_id": 5854, "action_date": "2014-02-10T10:00:00-06:00", "document_id": 29980, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/29980.pdf", "status_text": "Scheduled for hearing", "journal_page": 0, "committee_id_action": 217, "committee_id_assigned": null, "result": "N", "vote": {}}, {"bill_id": 5854, "action_date": "2014-02-12T10:00:00-06:00", "document_id": 30099, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/30099.pdf", "status_text": "Scheduled for hearing", "journal_page": 0, "committee_id_action": 217, "committee_id_assigned": null, "result": "N", "vote": {}}, {"bill_id": 5854, "action_date": "2014-02-12T10:00:00-06:00", "document_id": 30271, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/30271.pdf", "status_text": "Do Pass", "journal_page": 389, "committee_id_action": 217, "committee_id_assigned": null, "result": "P", "vote": {"vote_id": 15993, "president_vote": null, "Yea": [528, 522, 467, 425, 451, 845, 487, 497, 518, 450, 506, 508], "Nay": [502]}}, {"bill_id": 5854, "action_date": "2014-02-13T13:00:00-06:00", "document_id": 30400, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/30400.pdf", "status_text": "Deferred to another day", "journal_page": 416, "committee_id_action": 231, "committee_id_assigned": null, "result": "P", "vote": {}}, {"bill_id": 5854, "action_date": "2014-02-18T14:00:00-06:00", "document_id": 30510, "document_url": "https://mylrc.sdlegislature.gov/api/D