, {"meeting_datetime": "2019-02-26T00:00:00-06:00", "committee": "SEN", "url": "https://sdpb.sd.gov/sdpbpodcast/2019/sen30.mp3", "start_seconds": 820.0}, {"meeting_datetime": "2019-02-26T00:00:00-06:00", "committee": "SEN", "url": "https://sdpb.sd.gov/sdpbpodcast/2019/sen30.mp3", "start_seconds": 820.0}], "bill_versions": [{"bill_id": 9852, "bill_version_id": 52782, "bill_version": "Introduced", "bill_version_date": "2019-01-30T10:19:57.697-06:00", "bill_text": "FOR AN ACT ENTITLED, An Act to revise provisions regarding the posting of county and municipality ordinances. BE IT ENACTED BY THE LEGISLATURE OF THE STATE OF SOUTH DAKOTA: Section 1. That chapter 7-18A be amended by adding a NEW SECTION to read: If a county posts the ordinance book or any part of the book on the county's official website, the county shall ensure the most current version of the ordinance book or any part of the book is posted. The county auditor shall file an affidavit of online publication with the original ordinance. Section 2. That chapter 9-19 be amended by adding a NEW SECTION to read: If a municipality posts the ordinance book or any part of the book on the municipality's official website, the municipality shall ensure the most current version of the ordinance book or any part of the book is posted. The auditor or clerk shall file an affidavit of online publication with the original ordinance. 100 copies were printed on recycled paper by the South Dakota Legislative Research Council at a cost of $.167 per page. . Insertions into existing statutes are indicated by underscores. Deletions from existing statutes are indicated by overstrikes."}, {"bill_id": 9852, "bill_version_id": 54101, "bill_version": "Senate Local Government Engrossed", "bill_version_date": "2019-02-22T10:09:50.537-06:00", "bill_text": "FOR AN ACT ENTITLED, An Act to revise provisions regarding the posting of county and municipality ordinances. BE IT ENACTED BY THE LEGISLATURE OF THE STATE OF SOUTH DAKOTA: Section 1. That chapter 7-18A be amended by adding a NEW SECTION to read: If a county posts the ordinance book or any part of the book on the county's official website, the county shall ensure the most current version of the ordinance book or any part of the book is posted. Section 2. That chapter 9-19 be amended by adding a NEW SECTION to read: If a municipality posts the ordinance book or any part of the book on the municipality's official website, the municipality shall ensure the most current version of the ordinance book or any part of the book is posted. 100 copies were printed on recycled paper by the South Dakota Legislative Research Council at a cost of $.167 per page. . Insertions into existing statutes are indicated by underscores. Deletions from existing statutes are indicated by overstrikes."}, {"bill_id": 9852, "bill_version_id": 54547, "bill_version": "Enrolled", "bill_version_date": "2019-03-04T10:52:49.98-06:00", "bill_text": ""}], "amendments": [], "fiscal_notes": [], "action_log": [{"bill_id": 9852, "action_date": "2019-01-30T14:00:00-06:00", "document_id": 52890, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/52890.pdf", "status_text": "First read in House and referred to", "journal_page": 256, "committee_id_action": 408, "committee_id_assigned": 426, "result": "N", "vote": {}}, {"bill_id": 9852, "action_date": "2019-02-07T10:00:00-06:00", "document_id": 53043, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/53043.pdf", "status_text": "Scheduled for hearing", "journal_page": 0, "committee_id_action": 426, "committee_id_assigned": null, "result": "N", "vote": {}}, {"bill_id": 9852, "action_date": "2019-02-07T10:00:00-06:00", "document_id": 53227, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/53227.pdf", "status_text": "Do Pass", "journal_page": 320, "committee_id_action": 426, "committee_id_assigned": null, "result": "P", "vote": {"vote_id": 27922, "president_vote": null, "Yea": [1619, 1641, 1629, 1440, 1573, 165