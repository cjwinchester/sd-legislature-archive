: 0.0}], "bill_versions": [{"bill_id": 16208, "bill_version_id": 88238, "bill_version": "Introduced", "bill_version_date": "2001-01-23T08:21:39-06:00", "bill_text": "FOR AN ACT ENTITLED, An Act to make the refund the payment of certain inheritance tax collections. BE IT ENACTED BY THE LEGISLATURE OF THE STATE OF SOUTH DAKOTA: Section 1. The provisions of chapter 10-40 do not apply to the transfer of any real or personal property from any person who dies on or after November 7, 2000. Section 2. The Department of Revenue shall refund any tax paid pursuant to chapter 10-40 that is now exempt pursuant to section 1 of this Act. 450 copies of this document were printed by the South Dakota Legislative Research Council at a cost of $.021 per page. . Insertions into existing statutes are indicated by underscores. Deletions from existing statutes are indicated by overstrikes."}], "amendments": [], "fiscal_notes": [], "action_log": [{"bill_id": 16208, "action_date": "2001-01-23T14:00:00-06:00", "document_id": 88299, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/88299.pdf", "status_text": "First read in Senate and referred to", "journal_page": 170, "committee_id_action": 713, "committee_id_assigned": 696, "result": "N", "vote": {}}, {"bill_id": 16208, "action_date": "2001-01-29T08:00:00-06:00", "document_id": 88825, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/88825.pdf", "status_text": "Scheduled for Committee hearing on this date", "journal_page": null, "committee_id_action": 696, "committee_id_assigned": null, "result": "N", "vote": {}}, {"bill_id": 16208, "action_date": "2001-01-29T08:00:00-06:00", "document_id": 88825, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/88825.pdf", "status_text": "Title Amended", "journal_page": null, "committee_id_action": 696, "committee_id_assigned": null, "result": "P", "vote": {}}, {"bill_id": 16208, "action_date": "2001-01-29T08:00:00-06:00", "document_id": 88825, "document_url": "https:/