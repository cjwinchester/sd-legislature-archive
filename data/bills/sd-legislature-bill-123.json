tor vehicles, as defined in \u00a7 32-3-1(11), not to exceed a total cumulative value of five thousand dollars, per debtor."}], "amendments": [], "fiscal_notes": [], "action_log": [{"bill_id": 123, "action_date": "2013-01-22T14:00:00-06:00", "document_id": 24266, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/24266.pdf", "status_text": "First read in House and referred to", "journal_page": 132, "committee_id_action": 202, "committee_id_assigned": 198, "result": "N", "vote": {}}, {"bill_id": 123, "action_date": "2013-01-28T10:00:00-06:00", "document_id": 24621, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/24621.pdf", "status_text": "Scheduled for hearing", "journal_page": 0, "committee_id_action": 198, "committee_id_assigned": null, "result": "N", "vote": {}}, {"bill_id": 123, "action_date": "2013-01-28T10:00:00-06:00", "document_id": 24798, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/24798.pdf", "status_text": "Do Pass", "journal_page": 0, "committee_id_action": 198, "committee_id_assigned": null, "result": "F", "vote": {"vote_id": 13023, "president_vote": null, "Nay": [353, 350, 369, 359, 398, 406, 416, 402, 381, 334], "Yea": [351, 324, 348]}}, {"bill_id": 123, "action_date": "2013-01-28T10:00:00-06:00", "document_id": 24798, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/24798.pdf", "status_text": "Deferred to the 41st legislative day", "journal_page": 205, "committee_id_action": 198, "committee_id_assigned": null, "result": "P", "vote": {"vote_id": 13024, "president_vote": null, "Yea": [353, 350, 369, 359, 398, 406, 416, 402, 381, 334], "Nay": [351, 324, 348]}}]}