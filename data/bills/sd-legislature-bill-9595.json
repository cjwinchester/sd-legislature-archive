ully expended or obligated shall revert in accordance with the procedures prescribed in chapter 4-8."}], "amendments": [], "fiscal_notes": [], "action_log": [{"bill_id": 9595, "action_date": "2019-01-23T14:00:00-06:00", "document_id": 52083, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/52083.pdf", "status_text": "First read in House and referred to", "journal_page": 158, "committee_id_action": 408, "committee_id_assigned": 429, "result": "N", "vote": {}}, {"bill_id": 9595, "action_date": "2019-01-29T08:00:00-06:00", "document_id": 52177, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/52177.pdf", "status_text": "Scheduled for hearing", "journal_page": 0, "committee_id_action": 429, "committee_id_assigned": null, "result": "N", "vote": {}}, {"bill_id": 9595, "action_date": "2019-01-29T08:00:00-06:00", "document_id": 52616, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/52616.pdf", "status_text": "Recommend Referral to", "journal_page": 231, "committee_id_action": 429, "committee_id_assigned": 438, "result": "P", "vote": {"vote_id": 27559, "president_vote": null, "Yea": [1643, 1613, 1440, 1624, 1649, 1632, 1612, 1577, 1576, 1610, 1580, 1564, 1427]}}, {"bill_id": 9595, "action_date": "2019-01-29T14:00:00-06:00", "document_id": 52673, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/52673.pdf", "status_text": "Referred to", "journal_page": 232, "committee_id_action": 408, "committee_id_assigned": 438, "result": "P", "vote": {}}, {"bill_id": 9595, "action_date": "2019-02-26T08:00:00-06:00", "document_id": 54126, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/54126.pdf", "status_text": "Scheduled for hearing", "journal_page": 0, "committee_id_action": 438, "committee_id_assigned": null, "result": "N", "vote": {}}, {"bill_id": 9595, "action_date": "2019-02-26T08:00:00-06:00", "document_id": 54284, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/54284.pdf", "status_text": "Deferred