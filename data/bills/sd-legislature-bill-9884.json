18.0}], "bill_versions": [{"bill_id": 9884, "bill_version_id": 52724, "bill_version": "Introduced", "bill_version_date": "2019-01-30T09:25:57.163-06:00", "bill_text": "FOR AN ACT ENTITLED, An Act to provide for a period to cure certain campaign finance violations. BE IT ENACTED BY THE LEGISLATURE OF THE STATE OF SOUTH DAKOTA: Section 1. That chapter 12-27 be amended by adding a NEW SECTION to read: Prior to bringing a charge against any person who is subject to a Class 2 misdemeanor under this chapter, the agent of the state who is bringing the charge shall provide the person with a period, not in excess of seven days, during which the person may cure the violation. 100 copies were printed on recycled paper by the South Dakota Legislative Research Council at a cost of $.167 per page. . Insertions into existing statutes are indicated by underscores. Deletions from existing statutes are indicated by overstrikes."}, {"bill_id": 9884, "bill_version_id": 54820, "bill_version": "Enrolled", "bill_version_date": "2019-03-07T09:46:11.667-06:00", "bill_text": ""}], "amendments": [], "fiscal_notes": [], "action_log": [{"bill_id": 9884, "action_date": "2019-01-30T14:00:00-06:00", "document_id": 52890, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/52890.pdf", "status_text": "First read in House and referred to", "journal_page": 250, "committee_id_action": 408, "committee_id_assigned": 424, "result": "N", "vote": {}}, {"bill_id": 9884, "action_date": "2019-02-13T07:45:00-06:00", "document_id": 53407, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/53407.pdf", "status_text": "Scheduled for hearing", "journal_page": 0, "committee_id_action": 424, "committee_id_assigned": null, "result": "N", "vote": {}}, {"bill_id": 9884, "action_date": "2019-02-13T07:45:00-06:00", "document_id": 53689, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/53689.pdf", "status_text": "Do Pass", "journal_page": 407, "committee_id_action": 424, "committee_id_as