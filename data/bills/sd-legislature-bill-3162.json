ill_versions": [{"bill_id": 3162, "bill_version_id": 19076, "bill_version": "Introduced", "bill_version_date": "2012-01-05T15:16:17.25-06:00", "bill_text": "FOR AN ACT ENTITLED, An Act to repeal certain obsolete or unnecessary provisions pertaining to the Department of Game, Fish and Parks. BE IT ENACTED BY THE LEGISLATURE OF THE STATE OF SOUTH DAKOTA: Section 1. That \u00a7 41-2-18.1 be repealed. 41-2-18.1. For the purpose of implementing the pheasants-for-everyone program, the Game, Fish and Parks Commission may adopt reasonable rules to specify eligibility criteria, application procedures and establish standards for distribution of the grant funds. The rules shall be promulgated in accordance with chapter 1-26. Section 2. That \u00a7 41-6-69 be repealed. 41-6-69. Not later than the last day of January of each year, each agent shall return to the Department of Game, Fish and Parks all unused licenses, together with the stubs or duplicate copies of all licenses issued. Following the expiration date of hunting, fishing, and trapping licenses, stamps, and permits issued by the department, the department may destroy the licenses, stamps, and permits together with the stubs or duplicate copies upon completion of an audit thereof by the Department of Legislative Audit and filing of a satisfactory report. Section 3. That \u00a7 41-8-5 be repealed. 41-8-5. The Department of Game, Fish and Parks may not authorize the taking of more than forty mountain goats within one year. Section 4. That \u00a7 41-14-34 be repealed. 41-14-34. It is a Class 1 misdemeanor for any person to barter, trade, or sell baitfish within South Dakota, or to export baitfish from South Dakota, unless the person is licensed by the Game, Fish and Parks Department in accordance with the provisions of this chapter. Section 5. That \u00a7 41-17-3 be repealed. 41-17-3. The Department of Game, Fish and Parks shall maintain in its office in Pierre current maps showing the location and boundaries of each unit of the state park system as defined by \u00a7 41-17-1.1."}, {"bill_id": 3162, "bill_version_id": 22039, "bill_version": "Enrolled", "bill_version_date": "2012-02-24T09:55:30.16-06:00", "bill_text": ""}], "amendments": [], "fiscal_notes": [], "action_log": [{"bill_id": 3162, "action_date": "2012-01-10T12:00:00-06:00", "document_id": 19242, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/19242.pdf", "status_text": "First read in House and referred to", "journal_page": 18, "committee_id_action": 173, "committee_id_assigned": 174, "result": "N", "vote": {}}, {"bill_id": 3162, "action_date": "2012-01-26T07:45:00-06:00", "document_id": 20042, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/20042.pdf", "status_text": "Scheduled for hearing", "journal_page": 0, "committee_id_action": 174, "committee_id_assigned": null, "result": "N", "vote": {}}, {"bill_id": 3162, "action_date": "2012-01-26T07:45:00-06:00", "document_id": 20233, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/20233.pdf", "status_text": "Do Pass", "journal_page": 212, "committee_id_action": 174, "committee_id_assigned": null, "result": "P", "vote": {"vote_id": 10548, "president_vote": null, "Yea": [242, 230, 280, 241, 308, 273, 315, 282, 216, 306, 275, 270], "Excused": [238]}}, {"bill_id": 3162, "action_date": "2012-01-26T07:45:00-06:00", "document_id": 20233, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/20233.pdf", "status_text": "Placed on consent", "journal_page": 0, "committee_id_action": 174, "committee_id_assigned": null, "result": "P", "vote": {}}, {"bill_id": 3162, "action_date": "2012-01-30T14:00:00-06:00", "document_id": 20410, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/20410.pdf", "status_text": "Do Pass", "journal_page": 239, "committee_id_action": 173, "committee_id_assigned": null, "result": "P", "vote": {"vote_id": 10689, "president_vote": null, "Yea": [222, 262, 265, 227, 242, 231, 281, 269, 296, 211, 314, 257,