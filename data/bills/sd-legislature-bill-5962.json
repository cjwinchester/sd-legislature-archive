, "bill_versions": [{"bill_id": 5962, "bill_version_id": 29716, "bill_version": "Introduced", "bill_version_date": "2014-02-04T10:24:01.803-06:00", "bill_text": "FOR AN ACT ENTITLED, An Act to increase the maximum bet limit and to eliminate the award limit on video lottery games. BE IT ENACTED BY THE LEGISLATURE OF THE STATE OF SOUTH DAKOTA: Section 1. That \u00a7 42-7A-38 be amended to read as follows: 42-7A-38. A video lottery machine may not allow more than two five dollars to be played on a game or award free games or credits in excess of one thousand dollars per bet. The payback value of one credit shall be at least eighty percent of the value of the credit. 170 copies were printed on recycled paper by the South Dakota Legislative Research Council at a cost of $.098 per page. . Insertions into existing statutes are indicated by underscores. Deletions from existing statutes are indicated by overstrikes."}, {"bill_id": 5962, "bill_version_id": 30288, "bill_version": "House Commerce and Energy Engrossed", "bill_version_date": "2014-02-12T13:23:08.31-06:00", "bill_text": "FOR AN ACT ENTITLED, An Act to increase the maximum bet limit and to eliminate the award limit on video lottery games. BE IT ENACTED BY THE LEGISLATURE OF THE STATE OF SOUTH DAKOTA: Section 1. That \u00a7 42-7A-38 be amended to read as follows: 42-7A-38. A video lottery machine may not allow more than two dollars to be played on a game or award free games or credits in excess of one thousand dollars per bet. The commission shall promulgate rules, pursuant to chapter 1-26, establishing the maximum amount of money, not to exceed five dollars, that may be bet per play on a video lottery machine, the maximum award per play, and the award of free games or credits. The payback value of one credit shall be at least eighty percent of the value of the credit. 170 copies were printed on recycled paper by the South Dakota Legislative Research Council at a cost of $.098 per page. . Insertions into existing statutes are indicated by underscores. Deletions from existing statutes are indicated by overstrikes."}], "amendments": [], "fiscal_notes": [], "action_log": [{"bill_id": 5962, "action_date": "2014-02-04T14:00:00-06:00", "document_id": 29776, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/29776.pdf", "status_text": "First read in House and referred to", "journal_page": 272, "committee_id_action": 231, "committee_id_assigned": 217, "result": "N", "vote": {}}, {"bill_id": 5962, "action_date": "2014-02-12T10:00:00-06:00", "document_id": 30099, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/30099.pdf", "status_text": "Scheduled for hearing", "journal_page": 0, "committee_id_action": 217, "committee_id_assigned": null, "result": "N", "vote": {}}, {"bill_id": 5962, "action_date": "2014-02-12T10:00:00-06:00", "document_id": 30271, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/30271.pdf", "status_text": "Motion to amend", "journal_page": 0, "committee_id_action": 217, "committee_id_assigned": null, "result": "P", "vote": {}}, {"bill_id": 5962, "action_date": "2014-02-12T10:00:00-06:00", "document_id": 30271, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/30271.pdf", "status_text": "Do Pass Amended", "journal_page": 389, "committee_id_action": 217, "committee_id_assigned": null, "result": "P", "vote": {"vote_id": 16011, "president_vote": null, "Nay": [528, 522, 487, 506], "Yea": [467, 425, 451, 845, 502, 497, 518, 450, 508]}}, {"bill_id": 5962, "action_date": "2014-02-18T14:00:00-06:00", "document_id": 30510, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/30510.pdf", "status_text": "Motion to amend", "journal_page": 439, "committee_id_action": 231, "committee_id_assigned": null, "result": "P", "vote": {}}, {"bill_id": 5962, "action_date": "2014-02-18T14:00:00-06:00", "document_id": 30510, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/30510.pdf", "status_text": "Motion to amend", "