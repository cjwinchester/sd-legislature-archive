econds": 0.0}, {"meeting_datetime": "2001-02-22T00:00:00-06:00", "committee": "HAG", "url": "https://sdpb.sd.gov/sdpbpodcast/2001/hag31sb0190a.mp3", "start_seconds": 0.0}, {"meeting_datetime": "2001-02-22T00:00:00-06:00", "committee": "HAG", "url": "https://sdpb.sd.gov/sdpbpodcast/2001/hag31sb0190a.mp3", "start_seconds": 0.0}], "bill_versions": [{"bill_id": 16296, "bill_version_id": 88540, "bill_version": "Introduced", "bill_version_date": "2001-01-25T09:44:50-06:00", "bill_text": "FOR AN ACT ENTITLED, An Act to provide for an early pheasant season for resident hunters. BE IT ENACTED BY THE LEGISLATURE OF THE STATE OF SOUTH DAKOTA: Section 1. That chapter 41-6 be amended by adding thereto a NEW SECTION to read as follows: The Game, Fish and Parks Commission shall promulgate, pursuant to chapter 1-26, rules to establish an early pheasant hunting season for resident hunters only. The rules shall provide for shooting hours, daily bag limits, and possession limits. The early pheasant season shall be for three consecutive days beginning the Wednesday before the third Saturday of October. Any hunting during this season shall be limited to publicly-owned land and to privately-owned land leased by the Department of Game, Fish and Parks for public hunting purposes. 450 copies of this document were printed by the South Dakota Legislative Research Council at a cost of $.021 per page. . Insertions into existing statutes are indicated by underscores. Deletions from existing statutes are indicated by overstrikes."}, {"bill_id": 16296, "bill_version_id": 89408, "bill_version": "Senate Agriculture and Natural Resources Engrossed", "bill_version_date": "2001-02-06T15:24:35-06:00", "bill_text": "FOR AN ACT ENTITLED, An Act to provide for an early pheasant season for resident hunters. BE IT ENACTED BY THE LEGISLATURE OF THE STATE OF SOUTH DAKOTA: Section 1. That chapter 41-6 be amended by adding thereto a NEW SECTION to read as follows: The Game, Fish and Parks Commission shall promulgate, pursuant to chapter 1-26, rules to establish an early pheasant hunting season for resident hunters only. The rules shall provide for shooting hours, daily bag limits, and possession limits. The early pheasant season shall be for three consecutive days. 450 copies of this document were printed by the South Dakota Legislative Research Council at a cost of $.021 per page. . Insertions into existing statutes are indicated by underscores. Deletions from existing statutes are indicated by overstrikes."}], "amendments": [], "fiscal_notes": [], "action_log": [{"bill_id": 16296, "action_date": "2001-01-25T14:00:00-06:00", "document_id": 88645, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/88645.pdf", "status_text": "First read in Senate and referred to", "journal_page": 220, "committee_id_action": 713, "committee_id_assigned": 686, "result": "N", "vote": {}}, {"bill_id": 16296, "action_date": "2001-02-01T10:00:00-06:00", "document_id": 89151, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/89151.pdf", "status_text": "Scheduled for Committee hearing on this date", "journal_page": null, "committee_id_action": 686, "committee_id_assigned": null, "result": "N", "vote": {}}, {"bill_id": 16296, "action_date": "2001-02-01T10:00:00-06:00", "document_id": 89151, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/89151.pdf", "status_text": "Deferred to another day", "journal_page": null, "committee_id_action": 686, "committee_id_assigned": null, "result": "P", "vote": {}}, {"bill_id": 16296, "action_date": "2001-02-06T10:00:00-06:00", "document_id": 89384, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/89384.pdf", "status_text": "Scheduled for Committee hearing on this date", "journal_page": null, "committee_id_action": 686, "committee_id_assigned": null, "result": "N", "vote": {}}, {"bill_id": 16296, "action_date": "2001-02-06T10:00:00-06:00", "document_id": 89384, "document_url": "https://mylrc.sdlegislature.gov/api/D