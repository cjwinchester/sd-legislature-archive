ds": 0.0}, {"meeting_datetime": "2003-02-05T00:00:00-06:00", "committee": "HTR", "url": "https://sdpb.sd.gov/sdpbpodcast/2003/htr16.mp3", "start_seconds": 0.0}], "bill_versions": [{"bill_id": 17719, "bill_version_id": 95272, "bill_version": "Introduced", "bill_version_date": "2003-01-03T15:12:52-06:00", "bill_text": "FOR AN ACT ENTITLED, An Act to permit the Department of Social Services to publish certain fee schedules used in certain medical assistance programs. BE IT ENACTED BY THE LEGISLATURE OF THE STATE OF SOUTH DAKOTA: Section 1. That chapter 28-6 be amended by adding thereto a NEW SECTION to read as follows: The Department of Social Services may establish or amend fee schedules used to pay for items and services covered by the medical assistance program under the provisions of this chapter without following the procedures set forth in \u00a7 \u00a7 1-26-4 to 1-26-6, inclusive. The department shall post the fee schedules on the department's website and shall notify website users of pending changes by posting a notice of the pending changes on the website at least forty-eight hours before the changes are made. 350 copies of this document were printed by the South Dakota Legislative Research Council at a cost of $.027 per page. . Insertions into existing statutes are indicated by underscores. Deletions from existing statutes are indicated by overstrikes."}, {"bill_id": 17719, "bill_version_id": 95735, "bill_version": "House Health and Human Services Engrossed", "bill_version_date": "2003-01-24T14:01:39-06:00", "bill_text": "FOR AN ACT ENTITLED, An Act to permit the Department of Social Services to publish certain fee schedules used in certain medical assistance programs. BE IT ENACTED BY THE LEGISLATURE OF THE STATE OF SOUTH DAKOTA: Section 1. That chapter 28-6 be amended by adding thereto a NEW SECTION to read as follows: The Department of Social Services may establish or amend fee schedules used to pay for items and services covered by the medical assistance program under the provisions of this chapter without following the procedures set forth in \u00a7 \u00a7 1-26-4 to 1-26-6, inclusive. The department shall post the fee schedules on the department's website and shall notify website users of pending changes by posting a notice of the pending changes on the website at least forty-eight hours before the changes are made. Fee schedules posted on the department's website must be in accordance with the department's administrative rules promulgated under the provisions of chapter 1-26 which establish the reimbursement provisions for medical services. 350 copies of this document were printed by the South Dakota Legislative Research Council at a cost of $.027 per page. . Insertions into existing statutes are indicated by underscores. Deletions from existing statutes are indicated by overstrikes."}, {"bill_id": 17719, "bill_version_id": 96763, "bill_version": "Enrolled", "bill_version_date": "2003-02-07T08:33:25-06:00", "bill_text": ""}], "amendments": [], "fiscal_notes": [], "action_log": [{"bill_id": 17719, "action_date": "2003-01-14T12:00:00-06:00", "document_id": 95377, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/95377.pdf", "status_text": "First read in House and referred to", "journal_page": 15, "committee_id_action": 798, "committee_id_assigned": 789, "result": "N", "vote": {}}, {"bill_id": 17719, "action_date": "2003-01-24T07:45:00-06:00", "document_id": 95685, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/95685.pdf", "status_text": "Do Pass Amended,", "journal_page": 142, "committee_id_action": 789, "committee_id_assigned": null, "result": "P", "vote": {"vote_id": 55518, "president_vote": null, "Yea": [2843, 2871, 2872, 2888, 2896, 2915, 2930, 2933, 2859, 2932], "Excused": [2862, 2900, 2934]}}, {"bill_id": 17719, "action_date": "2003-01-24T07:45:00-06:00", "document_id": 95685, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/95685.pdf", "status_text": "Place on Consen