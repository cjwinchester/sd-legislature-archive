, {"meeting_datetime": "2013-01-31T00:00:00-06:00", "committee": "HOU", "url": "https://sdpb.sd.gov/sdpbpodcast/2013/hou16.mp3", "start_seconds": 5935.0}, {"meeting_datetime": "2013-01-31T00:00:00-06:00", "committee": "HOU", "url": "https://sdpb.sd.gov/sdpbpodcast/2013/hou16.mp3", "start_seconds": 5935.0}], "bill_versions": [{"bill_id": 4082, "bill_version_id": 23426, "bill_version": "Introduced", "bill_version_date": "2013-01-05T16:59:20.633-06:00", "bill_text": "FOR AN ACT ENTITLED, An Act to increase the penalty for sexual acts between juvenile correction facility employees and detainees. BE IT ENACTED BY THE LEGISLATURE OF THE STATE OF SOUTH DAKOTA: Section 1. That \u00a7 22-22-7.6 be amended to read as follows: 22-22-7.6. Any person employed at any jail or juvenile correctional facility, who knowingly engages in an act of sexual contact or sexual penetration with another person who is in detention and under the custodial, supervisory, or disciplinary authority of the person so engaging, and which act of sexual contact or sexual penetration does not otherwise constitute a felony pursuant to the provisions of chapter 22-22, is guilty of a Class 6 felony if the victim is an adult and a Class 4 felony if the victim is a juvenile. A juvenile correctional facility pursuant to this section is a juvenile detention facility as defined in subdivision 26-7A-1(16) or a juvenile facility operated by the Department of Corrections under \u00a7 1-15-1.4. 170 copies were printed on recycled paper by the South Dakota Legislative Research Council at a cost of $.093 per page. . Insertions into existing statutes are indicated by underscores. Deletions from existing statutes are indicated by overstrikes."}, {"bill_id": 4082, "bill_version_id": 23769, "bill_version": "Senate Judiciary Engrossed", "bill_version_date": "2013-01-15T12:04:24.133-06:00", "bill_text": "FOR AN ACT ENTITLED, An Act to increase the penalty for sexual acts between correction facility employees and juvenile detainees. BE IT ENACTED BY THE LEGISLATURE OF THE STATE OF SOUTH DAKOTA: Section 1. That \u00a7 22-22-7.6 be amended to read as follows: 22-22-7.6. Any person employed at any jail or juvenile correctional facility, who knowingly engages in an act of sexual contact or sexual penetration with another person who is in detention and under the custodial, supervisory, or disciplinary authority of the person so engaging, and which act of sexual contact or sexual penetration does not otherwise constitute a felony pursuant to the provisions of chapter 22-22, is guilty of a Class 6 felony if the victim is an adult and a Class 4 felony if the victim is a juvenile. A juvenile correctional facility pursuant to this section is a juvenile detention facility as defined in subdivision 26-7A-1(16) or a juvenile facility operated by the Department of Corrections under \u00a7 1-15-1.4. 170 copies were printed on recycled paper by the South Dakota Legislative Research Council at a cost of $.093 per page. . Insertions into existing statutes are indicated by underscores. Deletions from existing statutes are indicated by overstrikes."}, {"bill_id": 4082, "bill_version_id": 25128, "bill_version": "Enrolled", "bill_version_date": "2013-02-04T08:37:18.243-06:00", "bill_text": ""}], "amendments": [], "fiscal_notes": [], "action_log": [{"bill_id": 4082, "action_date": "2013-01-09T12:30:00-06:00", "document_id": 23601, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/23601.pdf", "status_text": "First read in Senate and referred to", "journal_page": 30, "committee_id_action": 200, "committee_id_assigned": 181, "result": "N", "vote": {}}, {"bill_id": 4082, "action_date": "2013-01-15T08:00:00-06:00", "document_id": 23620, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/23620.pdf", "status_text": "Scheduled for hearing", "journal_page": 0, "committee_id_action": 181, "committee_id_assigned": null, "result": "N", "vote": {}}, {"bill_id": 4082, "action_date": "2013-01-15T08: