, {"meeting_datetime": "2016-02-08T00:00:00-06:00", "committee": "HST", "url": "https://sdpb.sd.gov/sdpbpodcast/2016/hst17.mp3", "start_seconds": 1463.0}, {"meeting_datetime": "2016-02-10T00:00:00-06:00", "committee": "HOU", "url": "https://sdpb.sd.gov/sdpbpodcast/2016/hou19.mp3", "start_seconds": 3742.0}, {"meeting_datetime": "2016-02-10T00:00:00-06:00", "committee": "HOU", "url": "https://sdpb.sd.gov/sdpbpodcast/2016/hou19.mp3", "start_seconds": 3742.0}, {"meeting_datetime": "2016-02-10T00:00:00-06:00", "committee": "HOU", "url": "https://sdpb.sd.gov/sdpbpodcast/2016/hou19.mp3", "start_seconds": 3742.0}, {"meeting_datetime": "2016-02-16T00:00:00-06:00", "committee": "HOU", "url": "https://sdpb.sd.gov/sdpbpodcast/2016/hou21.mp3", "start_seconds": 2775.0}, {"meeting_datetime": "2016-02-16T00:00:00-06:00", "committee": "HOU", "url": "https://sdpb.sd.gov/sdpbpodcast/2016/hou21.mp3", "start_seconds": 2775.0}, {"meeting_datetime": "2016-02-16T00:00:00-06:00", "committee": "HOU", "url": "https://sdpb.sd.gov/sdpbpodcast/2016/hou21.mp3", "start_seconds": 2775.0}], "bill_versions": [{"bill_id": 7086, "bill_version_id": 38625, "bill_version": "Introduced", "bill_version_date": "2016-02-03T09:46:25.843-06:00", "bill_text": "FOR AN ACT ENTITLED, An Act to provide access to abandoned cemeteries and private burying grounds. BE IT ENACTED BY THE LEGISLATURE OF THE STATE OF SOUTH DAKOTA: Section 1. That the code be amended by adding a NEW SECTION to read: Any owner of private property on which a cemetery or grave is located shall allow ingress and egress to the cemetery or grave for purposes of basic lawn care, replacing and repairing of fences, corrective measures relative to grave markers, and visitation of graves. This right of access to the cemetery or grave extends for visitation or maintenance during reasonable hours, after obtaining consent of the landowner. No landowner may unreasonably withhold consent. The landowner shall designate the route for reasonable access. Section 2. That the code be amended by adding a NEW SECTION to read: A person entering private property to access a cemetery or grave under this Act is liable to the owner of the property for any damage caused as a result of the person's access. Section 3. That the code be amended by adding a NEW SECTION to read: A person denied reasonable access may bring an action in circuit court, where the property is located, to enjoin the owner of the property from denying the person reasonable ingress and egress to the cemetery or grave."}], "amendments": [], "fiscal_notes": [], "action_log": [{"bill_id": 7086, "action_date": "2016-02-03T14:00:00-06:00", "document_id": 38701, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/38701.pdf", "status_text": "First read in House and referred to", "journal_page": 258, "committee_id_action": 325, "committee_id_assigned": 317, "result": "N", "vote": {}}, {"bill_id": 7086, "action_date": "2016-02-08T07:45:00-06:00", "document_id": 38916, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/38916.pdf", "status_text": "Scheduled for hearing", "journal_page": 0, "committee_id_action": 317, "committee_id_assigned": null, "result": "N", "vote": {}}, {"bill_id": 7086, "action_date": "2016-02-08T07:45:00-06:00", "document_id": 38942, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/38942.pdf", "status_text": "Do Pass", "journal_page": 310, "committee_id_action": 317, "committee_id_assigned": null, "result": "P", "vote": {"vote_id": 20640, "president_vote": null, "Yea": [1042, 1022, 1002, 1044, 981, 1029, 989, 1025, 976], "Nay": [983, 1024, 1003, 990]}}, {"bill_id": 7086, "action_date": "2016-02-09T14:00:00-06:00", "document_id": 39095, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/39095.pdf", "status_text": "Deferred to another day", "journal_page": 350, "committee_id_action": 325, "committee_id_assigned": null, "result": "P", "vote": {}}, {"bill_id": 7086, "action_date": "2016-02-10T14:00:00-06: