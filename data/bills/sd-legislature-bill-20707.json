, {"meeting_datetime": "2006-02-08T00:00:00-06:00", "committee": "SJU", "url": "https://sdpb.sd.gov/sdpbpodcast/2006/sju21.mp3", "start_seconds": 2313.0}, {"meeting_datetime": "2006-02-15T00:00:00-06:00", "committee": "SJU", "url": "https://sdpb.sd.gov/sdpbpodcast/2006/sju26.mp3", "start_seconds": null}, {"meeting_datetime": "2006-02-15T00:00:00-06:00", "committee": "SJU", "url": "https://sdpb.sd.gov/sdpbpodcast/2006/sju26.mp3", "start_seconds": null}, {"meeting_datetime": "2006-02-15T00:00:00-06:00", "committee": "SJU", "url": "https://sdpb.sd.gov/sdpbpodcast/2006/sju26.mp3", "start_seconds": null}, {"meeting_datetime": "2006-02-17T00:00:00-06:00", "committee": "SEN", "url": "https://sdpb.sd.gov/sdpbpodcast/2006/sen28.mp3", "start_seconds": 3523.0}, {"meeting_datetime": "2006-02-17T00:00:00-06:00", "committee": "SEN", "url": "https://sdpb.sd.gov/sdpbpodcast/2006/sen28.mp3", "start_seconds": 3523.0}, {"meeting_datetime": "2006-02-17T00:00:00-06:00", "committee": "SEN", "url": "https://sdpb.sd.gov/sdpbpodcast/2006/sen28.mp3", "start_seconds": 3523.0}], "bill_versions": [{"bill_id": 20707, "bill_version_id": 106814, "bill_version": "Introduced", "bill_version_date": "2006-01-19T11:56:21-06:00", "bill_text": "FOR AN ACT ENTITLED, An Act to revise the definition of a prohibited sexual act. BE IT ENACTED BY THE LEGISLATURE OF THE STATE OF SOUTH DAKOTA: Section 1. That subdivision (16) of \u00a7 22-24A-2 be amended to read as follows: (16) \"Prohibited sexual act,\" actual or simulated sexual intercourse, sadism, masochism, sexual bestiality, incest, masturbation, or sadomasochistic abuse; actual or simulated exhibition of the genitals or , the pubic or rectal area , or the feminine breasts, in a lewd or lascivious manner; actual physical contact with a person's clothed or unclothed genitals, pubic area, buttocks, or, if such person is a female, breast with the intent to arouse or gratify the sexual desire of either party; defecation or urination for the purpose of creating sexual excitement in the viewer; or any act or conduct which constitutes sexual battery or simulates that sexual battery is being or will be committed. The term includes encouraging, aiding, abetting or enticing any person to commit any such acts as provided in this subdivision. The term does not include a mother's breast-feeding of her baby; 250 copies of this document were printed by the South Dakota Legislative Research Council at a cost of $.038 per page. . Insertions into existing statutes are indicated by underscores. Deletions from existing statutes are indicated by overstrikes."}, {"bill_id": 20707, "bill_version_id": 107728, "bill_version": "House Engrossed", "bill_version_date": "2006-02-01T16:09:17-06:00", "bill_text": "FOR AN ACT ENTITLED, An Act to revise the definition of a prohibited sexual act. BE IT ENACTED BY THE LEGISLATURE OF THE STATE OF SOUTH DAKOTA: Section 1. That subdivision (16) of \u00a7 22-24A-2 be amended to read as follows: (16) \"Prohibited sexual act,\" actual or simulated sexual intercourse, sadism, masochism, sexual bestiality, incest, masturbation, or sadomasochistic abuse; actual or simulated exhibition of the genitals or , the pubic or rectal area , or the bare feminine breasts, in a lewd or lascivious manner; actual physical contact with a person's clothed or unclothed genitals, pubic area, buttocks, or, if such person is a female, breast with the intent to arouse or gratify the sexual desire of either party; defecation or urination for the purpose of creating sexual excitement in the viewer; or any act or conduct which constitutes sexual battery or simulates that sexual battery is being or will be committed. The term includes encouraging, aiding, abetting or enticing any person to commit any such acts as provided in this subdivision. The term does not include a mother's breast-feeding of her baby; 250 copies of this document were printed by the South Dakota Legislative Research Council at a cost of $.038 per page. . Insertions into existing statutes are indicated by underscores. Deletions from existing statutes are indicated by overstrikes."}, {"bill_id": 20707, "bill_version_id": 109050, "bill_version": "Enrolled", "bill_version_date": "2006-02-21T09:20:17-06:00", "bill_text": ""}], "amendments": [], "fiscal_notes": [], "action_log": [{"bill_id": 20707, "action_date": "2006-01-19T14:00:00-06:00", "document_id": 106834, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/106834.pdf", "status_text": "First read in House and referred to", "journal_page": 147, "committee_id_action": 893, "committee_id_assigned": 891, "result": "N", "vote": {}}, {"bill_id": 20707, "action_date": "2006-01-30T07:30:00-06:00", "document_id": 107470, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/107470.pdf", "status_text": "Scheduled for Committee hearing on this date", "journal_page": null, "committee_id_action": 891, "committee_id_assigned": null, "result": "N", "vote": {}}, {"bill_id": 20707, "action_date": "2006-01-30T07:30:00-06:00", "document_id": 107470, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/107470.pdf", "status_text": "Do Pass,", "journal_page": 274, "committee_id_action": 891, "committee_id_assigned": null, "result": "P", "vote": {"vote_id": 63771, "president_vote": null, "Excused": [3387, 3461], "Yea": [3393, 3401, 3407, 3426, 3429, 3434, 3444, 3448, 3456, 3406, 3371]}}, {"bill_id": 20707, "action_date": "2006-01-31T14:00:00-06:00", "document_id": 107629, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/107629.pdf", "status_text": "Deferred to another day", "journal_page": 311, "committee_id_action": 893, "committee_id_assigned": null, "result": "P", "vote": {}}, {"bill_id": 20707, "action_date": "2006-02-01T14:00:00-06:00", "document_id": 107742, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/107742.pdf", "status_text": "Motion to Amend,", "journal_page": 332, "committee_id_action": 893, "committee_id_assigned": null,