ime": false}, {"legislator_profile_id": 2808, "is_prime": false}, {"legislator_profile_id": 2809, "is_prime": false}, {"legislator_profile_id": 2810, "is_prime": false}, {"legislator_profile_id": 2811, "is_prime": false}, {"legislator_profile_id": 2812, "is_prime": false}, {"legislator_profile_id": 2813, "is_prime": false}, {"legislator_profile_id": 2814, "is_prime": false}, {"legislator_profile_id": 2815, "is_prime": false}, {"legislator_profile_id": 2816, "is_prime": false}, {"legislator_profile_id": 2817, "is_prime": false}, {"legislator_profile_id": 2818, "is_prime": false}, {"legislator_profile_id": 2819, "is_prime": false}, {"legislator_profile_id": 2820, "is_prime": false}, {"legislator_profile_id": 2821, "is_prime": false}, {"legislator_profile_id": 2822, "is_prime": false}, {"legislator_profile_id": 2823, "is_prime": false}, {"legislator_profile_id": 2824, "is_prime": false}, {"legislator_profile_id": 2825, "is_prime": false}, {"legislator_profile_id": 2827, "is_prime": false}, {"legislator_profile_id": 2828, "is_prime": false}, {"legislator_profile_id": 2830, "is_prime": false}], "keywords": ["House Concurrent Resolutions"], "audio": [], "bill_versions": [{"bill_id": 17465, "bill_version_id": 92816, "bill_version": "Introduced", "bill_version_date": "2002-01-25T07:37:04-06:00", "bill_text": "A CONCURRENT RESOLUTION, Supporting the Kids Voting Program. WHEREAS, the citizens of our state and our nation are forever changed because of the terrorist attack on September 11, 2001; and WHEREAS, we stand resolved to build a foundation of democratic principles so that our nation will continue to grow and flourish; and WHEREAS, the need for Kids Voting is greater now than ever because we must make every effort, use every resource, and collaborate with every partner to educate our students about why it is so important to be active citizens, to be involved in their communities, to play active roles, and use their votes and their voices; and WHEREAS, we must prepare our youth to be leaders, voters, good citizens, and good neighbors; and WHEREAS, Kids Voting is a private, nonpartisan, nonprofit grassroots organization dedicated to building a better informed generation of voters and citizens active in their government; and WHEREAS, Kids Voting South Dakota strives to improve citizen involvement in democracy in South Dakota's communities by preparing current and future generations of citizens for participation in their government; and WHEREAS, research has proven that when students become involved in Kids Voting, their parents also become involved in their government and the election process: NOW, THEREFORE, BE IT RESOLVED, by the House of Representatives of the Seventy- seventh Legislature of the State of South Dakota, the Senate concurring therein, that the Legislature supports and endorses Kids Voting; and BE IT FURTHER RESOLVED, that January 23, 2002, be declared Kids Voting Day in South Dakota."}, {"bill_id": 17465, "bill_version_id": 93188, "bill_version": "Enrolled", "bill_version_date": "2002-01-30T10:15:33-06:00", "bill_text": ""}], "amendments": [], "fiscal_notes": [], "action_log": [{"bill_id": 17465, "action_date": "2002-01-26T13:00:00-06:00", "document_id": 92970, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/92970.pdf", "status_text": "Adopt Resolution", "journal_page": 301, "committee_id_action": 768, "committee_id_assigned": null, "result": "P", "vote": {"vote_id": 54013, "president_vote": null, "Yea": [2724, 2725, 2728, 2729, 2731, 2732, 2735, 2736, 2737, 2738, 2741, 2744, 2748, 2749, 2752, 2754, 2755, 2756, 2757, 2759, 2763, 2764, 2765, 2766, 2767, 2768, 2769, 2770, 2771, 2773, 2774, 2775, 2777, 2779, 2781, 2782, 2783, 2785, 2786, 2789, 2790, 2793, 2794, 2797, 2798, 2799, 2800, 2803, 2804, 2805, 2806, 2807, 2808, 2811, 2812, 2813, 2814, 2815, 2816, 2817, 2820, 2822, 2823, 2824, 2825, 2826, 2830, 2751], "Excused": [2758, 2788]}}, {"bill_id": 17465, "action_date": "2002-01-29T14:00:00-06:00", "documen