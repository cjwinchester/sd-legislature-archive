meanor."}, {"bill_id": 7969, "bill_version_id": 45126, "bill_version": "Enrolled", "bill_version_date": "2017-02-28T10:34:11.253-06:00", "bill_text": ""}], "amendments": [], "fiscal_notes": [], "action_log": [{"bill_id": 7969, "action_date": "2017-01-24T14:00:00-06:00", "document_id": 43064, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/43064.pdf", "status_text": "First read in Senate and referred to", "journal_page": 180, "committee_id_action": 360, "committee_id_assigned": 342, "result": "N", "vote": {}}, {"bill_id": 7969, "action_date": "2017-02-01T08:00:00-06:00", "document_id": 43189, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/43189.pdf", "status_text": "Scheduled for hearing", "journal_page": 0, "committee_id_action": 342, "committee_id_assigned": null, "result": "N", "vote": {}}, {"bill_id": 7969, "action_date": "2017-02-01T08:00:00-06:00", "document_id": 43510, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/43510.pdf", "status_text": "Do Pass", "journal_page": 246, "committee_id_action": 342, "committee_id_assigned": null, "result": "P", "vote": {"vote_id": 22703, "president_vote": null, "Yea": [1154, 1148, 1172, 1174, 1096, 1146, 1135]}}, {"bill_id": 7969, "action_date": "2017-02-02T14:00:00-06:00", "document_id": 43748, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/43748.pdf", "status_text": "Do Pass", "journal_page": 276, "committee_id_action": 360, "committee_id_assigned": null, "result": "P", "vote": {"vote_id": 22791, "president_vote": null, "Yea": [1154, 1171, 1103, 1148, 1108, 1096, 1097, 1109, 1105, 1175, 1100, 1106, 1164, 1194, 1169, 1107, 1104, 1163, 1099, 1144, 1131, 1135, 1178, 1157, 1172, 1151, 1174, 1146, 1102, 1098, 1137, 1167, 1132, 1189], "Excused": [1141]}}, {"bill_id": 7969, "action_date": "2017-02-03T13:00:00-06:00", "document_id": 43860, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/43860.pdf", "status_text": "First read in House and referred to", "jo