egislator_profile_id": 2391, "is_prime": false}, {"legislator_profile_id": 2392, "is_prime": false}, {"legislator_profile_id": 2393, "is_prime": false}, {"legislator_profile_id": 2394, "is_prime": false}, {"legislator_profile_id": 2396, "is_prime": false}, {"legislator_profile_id": 2397, "is_prime": false}, {"legislator_profile_id": 2398, "is_prime": false}, {"legislator_profile_id": 2399, "is_prime": false}, {"legislator_profile_id": 2400, "is_prime": false}, {"legislator_profile_id": 2401, "is_prime": false}, {"legislator_profile_id": 2402, "is_prime": false}, {"legislator_profile_id": 2403, "is_prime": false}, {"legislator_profile_id": 2404, "is_prime": false}, {"legislator_profile_id": 2405, "is_prime": false}, {"legislator_profile_id": 2406, "is_prime": false}, {"legislator_profile_id": 2407, "is_prime": false}], "keywords": ["Commemorations"], "audio": [], "bill_versions": [{"bill_id": 15764, "bill_version_id": 86716, "bill_version": "Introduced", "bill_version_date": "2000-01-27T00:00:00-06:00", "bill_text": "A LEGISLATIVE COMMEMORATION, Recognizing and commending Boy Scout Day on February 8, 2000, in South Dakota. WHEREAS, February 8, 2000, will be the ninetieth anniversary of Boy Scouts in America and the seventieth anniversary of the Black Hills Area Council in Rapid City; and WHEREAS, Boy Scouts of America is one of the oldest and largest youth serving organizations in America with four million members scouting nationwide; and WHEREAS, with 15,500 boy scouts from South Dakota, scouting tries to instill the principles of a strong community and moral values in our youth, along with character development, citizenship training, and personal fitness and responsibilities: NOW, THEREFORE, BE IT COMMEMORATED, by the Seventy-fifth Legislature of the State of South Dakota, that the Legislature commends the Boy Scouts of America and their outstanding character and leadership skills by designating February 8, 2000, as Boy Scout Day in South Dakota."}, {"bill_id": 15764, "bill_version_id": 86715, "bill_version": "Enrolled", "bill_version_date": "2000-02-01T00:00:00-06:00", "bill_text": "A LEGISLATIVE COMMEMORATION, Recognizing and commending Boy Scout Day on February 8, 2000, in South Dakota. WHEREAS, February 8, 2000, will be the ninetieth anniversary of Boy Scouts in America and the seventieth anniversary of the Black Hills Area Council in Rapid City; and WHEREAS, Boy Scouts of America is one of the oldest and largest youth serving organizations in America with four million members scouting nationwide; and WHEREAS, with 15,500 boy scouts from South Dakota, scouting tries to instill the principles of a strong community and moral values in our youth, along with character development, citizenship training, and personal fitness and responsibilities: NOW, THEREFORE, BE IT COMMEMORATED, by the Seventy-fifth Legislature of the State of South Dakota, that the Legislature commends the Boy Scouts of America and their outstanding character and leadership skills by designating February 8, 2000, as Boy Scout Day in South Dakota."}], "amendments": [], "fiscal_notes": [], "action_log": [{"bill_id": 15764, "action_date": "2000-01-27T14:00:00-06:00", "document_id": 85644, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/85644.pdf", "status_text": "", "journal_page": 279, "committee_id_action": 656, "committee_id_assigned": null, "result": "N", "vote": {}}, {"bill_id": 15764, "action_date": "2000-01-28T13:00:00-06:00", "document_id": 85576, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/85576.pdf", "status_text": "", "journal_page": null, "committee_id_action": 656, "committee_id_assigned": null, "result": "N", "vote": {}}, {"bill_id": 15764, "action_date": "2000-02-01T14:00:00-06:00", "document_id": 85651, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/85651.pdf", "status_text": "", "journal_page": null, "committee_id_action": 657, "committee_id_assigned": null, "result": "N", "vote": {}}], "rss_fe