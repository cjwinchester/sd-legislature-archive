, {"legislator_profile_id": 2825, "is_prime": false}, {"legislator_profile_id": 2826, "is_prime": false}, {"legislator_profile_id": 2828, "is_prime": false}], "keywords": [], "audio": [], "bill_versions": [{"bill_id": 17499, "bill_version_id": 94345, "bill_version": "Introduced", "bill_version_date": "2002-02-14T07:41:51-06:00", "bill_text": "A LEGISLATIVE COMMEMORATION, Honoring Darrell \"Brownie\" Eagle Bull for his commitment and achievements in the area of community education. WHEREAS, Darrell Eagle Bull has spent his career working for children and education in the Pine Ridge community; and WHEREAS, Darrell has set aside his own educational progress to work for students and family in the Twenty-first Century Program; and WHEREAS, by his sacrifices, Darrell has made the Twenty-first Century Program in Shannon County one of the most outstanding in the state by positively affecting over one hundred fifty children and their parents; and WHEREAS, Darrell's dedication has extended to after school programs and he has given up his own time for children and community: NOW, THEREFORE, BE IT COMMEMORATED, by the Seventy-seventh Legislature of the State of South Dakota, that Darrell \"Brownie\" Eagle Bull be honored for his copious dedication and service to his community, his tribe, and his state."}, {"bill_id": 17499, "bill_version_id": 94636, "bill_version": "Enrolled", "bill_version_date": "2002-02-19T18:35:50-06:00", "bill_text": ""}], "amendments": [], "fiscal_notes": [], "action_log": [{"bill_id": 17499, "action_date": "2002-02-14T14:00:00-06:00", "document_id": 94440, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/94440.pdf", "status_text": "", "journal_page": 680, "committee_id_action": 768, "committee_id_assigned": null, "result": "N", "vote": {}}, {"bill_id": 17499, "action_date": "2002-02-15T14:00:00-06:00", "document_id": 94518, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/94518.pdf", "status_text": "", "journal_page": null, "c