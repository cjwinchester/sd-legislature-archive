"bill_versions": [{"bill_id": 23371, "bill_version_id": 230890, "bill_version": "Introduced", "bill_version_date": "2022-01-27T10:53:10.787-06:00", "bill_text": "An Act to allow medical practice on the basis of conscience. Be it enacted by the Legislature of the State of South Dakota: Section 1. That a NEW SECTION be added to title 34: Notwithstanding any other provision of state law or rule, or federal law, regulation, or guidance, a medical practitioner has the right to practice in such manner that the practitioner deems is in the best interest of the practitioner's patient based on the practitioner's conscience."}], "amendments": [{"bill_id": 23371, "document_id": 233602, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/233602.pdf", "legislator_profile_id": 3947, "document_id_instructions": 233603, "document_id_instructions_url": "https://mylrc.sdlegislature.gov/api/Documents/233603.pdf"}], "fiscal_notes": [], "action_log": [{"bill_id": 23371, "action_date": "2022-01-27T14:00:00-06:00", "document_id": 230262, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/230262.pdf", "status_text": "First Reading House", "journal_page": 139, "committee_id_action": 1008, "committee_id_assigned": null, "result": "N", "vote": {}}, {"bill_id": 23371, "action_date": "2022-01-31T14:00:00-06:00", "document_id": 231012, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/231012.pdf", "status_text": "Referred to", "journal_page": 156, "committee_id_action": 1008, "committee_id_assigned": 1007, "result": "N", "vote": {}}, {"bill_id": 23371, "action_date": "2022-02-10T07:45:00-06:00", "document_id": 232905, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/232905.pdf", "status_text": "Scheduled for hearing", "journal_page": null, "committee_id_action": 1007, "committee_id_assigned": null, "result": "N", "vote": {}}, {"bill_id": 23371, "action_date": "2022-02-15T07:45:00-06:00", "document_id": 233222, "document_url": "https://mylrc.sdleg