art_seconds": 0.0}, {"meeting_datetime": "2001-01-29T00:00:00-06:00", "committee": "SJU", "url": "https://sdpb.sd.gov/sdpbpodcast/2001/sju14hb1061a.mp3", "start_seconds": 0.0}, {"meeting_datetime": "2001-01-29T00:00:00-06:00", "committee": "SJU", "url": "https://sdpb.sd.gov/sdpbpodcast/2001/sju14hb1061a.mp3", "start_seconds": 0.0}, {"meeting_datetime": "2001-01-29T00:00:00-06:00", "committee": "SJU", "url": "https://sdpb.sd.gov/sdpbpodcast/2001/sju14hb1061a.mp3", "start_seconds": 0.0}, {"meeting_datetime": "2001-01-30T00:00:00-06:00", "committee": "S", "url": "https://sdpb.sd.gov/sdpbpodcast/2001/sen15hb1061a.mp3", "start_seconds": 0.0}, {"meeting_datetime": "2001-01-30T00:00:00-06:00", "committee": "S", "url": "https://sdpb.sd.gov/sdpbpodcast/2001/sen15hb1061a.mp3", "start_seconds": 0.0}, {"meeting_datetime": "2001-01-30T00:00:00-06:00", "committee": "S", "url": "https://sdpb.sd.gov/sdpbpodcast/2001/sen15hb1061a.mp3", "start_seconds": 0.0}], "bill_versions": [{"bill_id": 16028, "bill_version_id": 87739, "bill_version": "Introduced", "bill_version_date": "2001-01-07T13:01:58-06:00", "bill_text": "FOR AN ACT ENTITLED, An Act to prohibit certain persons from applying for a hunting, fishing, or trapping license, to provide for an automatic revocation of any such license or privilege, and to provide for a penalty for a violation thereof. BE IT ENACTED BY THE LEGISLATURE OF THE STATE OF SOUTH DAKOTA: Section 1. That chapter 41-1 be amended by adding thereto a NEW SECTION to read as follows: No person who is subject to an unpaid and unsatisfied judgment in favor of the State of South Dakota for wildlife civil damages as determined in \u00a7 41-1-5.1 may purchase, attempt to purchase, or possess any South Dakota hunting, fishing, or trapping license in regard to the violation which initiated the civil judgment. If any person is in violation of this section, such person's applicable hunting, fishing, or trapping licenses and privileges are automatically revoked without hearing until the judgment is paid in full and satisfied. A violation of this section is a Class 1 misdemeanor. 450 copies of this document were printed by the South Dakota Legislative Research Council at a cost of $.021 per page. . Insertions into existing statutes are indicated by underscores. Deletions from existing statutes are indicated by overstrikes."}, {"bill_id": 16028, "bill_version_id": 87973, "bill_version": "House Judiciary Engrossed", "bill_version_date": "2001-01-17T13:08:23-06:00", "bill_text": "FOR AN ACT ENTITLED, An Act to prohibit certain persons from applying for a hunting, fishing, or trapping license, to provide for an automatic revocation of any license or privilege, and to provide for a penalty for a violation thereof. BE IT ENACTED BY THE LEGISLATURE OF THE STATE OF SOUTH DAKOTA: Section 1. That chapter 41-1 be amended by adding thereto a NEW SECTION to read as follows: No person who is subject to an unpaid and unsatisfied judgment in favor of the State of South Dakota for wildlife civil damages as determined in \u00a7 41-1-5.1 may purchase, attempt to purchase, or possess any South Dakota hunting, fishing, or trapping license. If any person is in violation of this section, such person's hunting, fishing, or trapping licenses and privileges are automatically revoked without hearing until the judgment is paid in full and satisfied. A violation of this section is a Class 1 misdemeanor. 450 copies of this document were printed by the South Dakota Legislative Research Council at a cost of $.021 per page. . Insertions into existing statutes are indicated by underscores. Deletions from existing statutes are indicated by overstrikes."}, {"bill_id": 16028, "bill_version_id": 89021, "bill_version": "Enrolled", "bill_version_date": "2001-01-31T09:42:40-06:00", "bill_text": ""}], "amendments": [], "fiscal_notes": [], "action_log": [{"bill_id": 16028, "action_date": "2001-01-09T14:00:00-06:00", "document_id": 87778, "document_url": "https://mylrc.sdlegisla