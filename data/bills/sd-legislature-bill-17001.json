start_seconds": 0.0}, {"meeting_datetime": "2002-02-04T00:00:00-06:00", "committee": "SRE", "url": "https://sdpb.sd.gov/sdpbpodcast/2002/sre20hb1093a.mp3", "start_seconds": 0.0}, {"meeting_datetime": "2002-02-04T00:00:00-06:00", "committee": "SRE", "url": "https://sdpb.sd.gov/sdpbpodcast/2002/sre20hb1093a.mp3", "start_seconds": 0.0}, {"meeting_datetime": "2002-02-04T00:00:00-06:00", "committee": "SRE", "url": "https://sdpb.sd.gov/sdpbpodcast/2002/sre20hb1093a.mp3", "start_seconds": 0.0}], "bill_versions": [{"bill_id": 17001, "bill_version_id": 91927, "bill_version": "Introduced", "bill_version_date": "2002-01-15T08:07:59-06:00", "bill_text": "FOR AN ACT ENTITLED, An Act to revise certain reemployment requirements in the South Dakota Retirement System. BE IT ENACTED BY THE LEGISLATURE OF THE STATE OF SOUTH DAKOTA: Section 1. That chapter 3-12 be amended by adding thereto a NEW SECTION to read as follows: Any member who is receiving a monthly benefit derived from that member's credited service and who is reemployed as a permanent full-time employee, shall make no additional contributions and is not eligible for future benefits pursuant to \u00a7 \u00a7 3-12-82 and 3-12-111. 350 copies of this document were printed by the South Dakota Legislative Research Council at a cost of $.027 per page. . Insertions into existing statutes are indicated by underscores. Deletions from existing statutes are indicated by overstrikes."}, {"bill_id": 17001, "bill_version_id": 92890, "bill_version": "House Retirement Laws Engrossed", "bill_version_date": "2002-01-25T16:35:01-06:00", "bill_text": "FOR AN ACT ENTITLED, An Act to revise certain reemployment requirements in the South Dakota Retirement System. BE IT ENACTED BY THE LEGISLATURE OF THE STATE OF SOUTH DAKOTA: Section 1. That chapter 3-12 be amended by adding thereto a NEW SECTION to read as follows: Any member who is receiving a monthly benefit derived from that member's credited service and who is reemployed as a permanent full-time employee, shall make no additional contributions and is not eligible for future benefits pursuant to \u00a7 \u00a7 3-12-82 and 3-12-111. This section shall not apply to any member who is reemployed as a permanent full-time employee or is party to an employment contract as a permanent full-time employee on or prior to the effective date of this Act. 350 copies of this document were printed by the South Dakota Legislative Research Council at a cost of $.027 per page. . Insertions into existing statutes are indicated by underscores. Deletions from existing statutes are indicated by overstrikes."}], "amendments": [], "fiscal_notes": [], "action_log": [{"bill_id": 17001, "action_date": "2002-01-15T14:00:00-06:00", "document_id": 92019, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/92019.pdf", "status_text": "First read in House and referred to", "journal_page": 115, "committee_id_action": 768, "committee_id_assigned": 762, "result": "N", "vote": {}}, {"bill_id": 17001, "action_date": "2002-01-24T15:00:00-06:00", "document_id": 92843, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/92843.pdf", "status_text": "Scheduled for Committee hearing on this date", "journal_page": null, "committee_id_action": 762, "committee_id_assigned": null, "result": "N", "vote": {}}, {"bill_id": 17001, "action_date": "2002-01-24T15:00:00-06:00", "document_id": 92843, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/92843.pdf", "status_text": "Do Pass Amended,", "journal_page": 277, "committee_id_action": 762, "committee_id_assigned": null, "result": "P", "vote": {"vote_id": 53450, "president_vote": null, "Yea": [2803, 2783, 2813, 2793], "Excused": [2741]}}, {"bill_id": 17001, "action_date": "2002-01-28T14:00:00-06:00", "document_id": 93059, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/93059.pdf", "status_text": "Do Pass Amended,", "journal_page": 327, "committee_id_action": 768, "committee_id_assigned": null, "result": "P", "