profile_id": 812, "is_prime": false}], "keywords": ["Property", "Public Property, Purchases and Contracts", "State Affairs and Government"], "audio": [{"meeting_datetime": "2010-02-01T00:00:00-06:00", "committee": "HST", "url": "https://sdpb.sd.gov/sdpbpodcast/2010/hst13.mp3", "start_seconds": 102.0}, {"meeting_datetime": "2010-02-01T00:00:00-06:00", "committee": "HST", "url": "https://sdpb.sd.gov/sdpbpodcast/2010/hst13.mp3", "start_seconds": 102.0}, {"meeting_datetime": "2010-02-01T00:00:00-06:00", "committee": "HST", "url": "https://sdpb.sd.gov/sdpbpodcast/2010/hst13.mp3", "start_seconds": 102.0}, {"meeting_datetime": "2010-02-01T00:00:00-06:00", "committee": "HST", "url": "https://sdpb.sd.gov/sdpbpodcast/2010/hst13.mp3", "start_seconds": 102.0}], "bill_versions": [{"bill_id": 1476, "bill_version_id": 10773, "bill_version": "Introduced", "bill_version_date": "2010-01-21T10:27:56.413-06:00", "bill_text": "FOR AN ACT ENTITLED, An Act to require state agencies to conduct a public hearing before acquiring certain real property. BE IT ENACTED BY THE LEGISLATURE OF THE STATE OF SOUTH DAKOTA: Section 1. Prior to the purchase of any real property by a state agency or the execution of a purchase agreement for such real property, the state agency shall conduct a public hearing to solicit input from the public and share information regarding the property to be purchased. The state agency shall notify the public of the hearing by publishing a notice in the official newspapers of the county in which the property to be acquired is located. The state agency shall publish the notice at least fifteen days before the hearing. The notice shall include the date, time, and location of the hearing and instructions for presenting oral and written comments to the state agency. The provisions of this section do not apply to any state agency which receives specific legislative approval to purchase the real property. 180 copies of this document were printed by the South Dakota Legislative Research Council at a cost of $.052 per page. . Insertions into existing statutes are indicated by underscores. Deletions from existing statutes are indicated by overstrikes."}], "amendments": [], "fiscal_notes": [], "action_log": [{"bill_id": 1476, "action_date": "2010-01-21T14:00:00-06:00", "document_id": 10821, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/10821.pdf", "status_text": "First read in House and referred to", "journal_page": 146, "committee_id_action": 86, "committee_id_assigned": 79, "result": "N", "vote": {}}, {"bill_id": 1476, "action_date": "2010-02-01T07:45:00-06:00", "document_id": 11145, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/11145.pdf", "status_text": "Scheduled for hearing", "journal_page": 0, "committee_id_action": 79, "committee_id_assigned": null, "result": "N", "vote": {}}, {"bill_id": 1476, "action_date": "2010-02-01T07:45:00-06:00", "document_id": 11443, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/11443.pdf", "status_text": "Motion to amend", "journal_page": 0, "committee_id_action": 79, "committee_id_assigned": null, "result": "P", "vote": {}}, {"bill_id": 1476, "action_date": "2010-02-01T07:45:00-06:00", "document_id": 11443, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/11443.pdf", "status_text": "Deferred to the 41st legislative day", "journal_page": 236, "committee_id_action": 79, "committee_id_assigned": null, "result": "P", "vote": {"vote_id": 5845, "president_vote": null, "Yea": [771, 798, 826, 800, 744, 786, 796, 748], "Nay": [756, 765, 788, 762], "Excused": [750]}}]}