, "bill_versions": [{"bill_id": 14660, "bill_version_id": 81090, "bill_version": "Introduced", "bill_version_date": "1999-01-27T00:00:00-06:00", "bill_text": "FOR AN ACT ENTITLED, An Act to require any person contesting a legislative election to first exhaust all statutory remedies. BE IT ENACTED BY THE LEGISLATURE OF THE STATE OF SOUTH DAKOTA: Section 1. That chapter 12-22 be amended by adding thereto a NEW SECTION to read as follows: Neither house of the Legislature, after notice to contest a legislative election pursuant to \u00a7 12-22-26, may proceed to judge the matter unless the body first finds, by motion, that the appellant has complied with all relevant state statutes in an effort to bring the election results to certainty. 750 copies of this document were printed by the South Dakota Legislative Research Council at a cost of $.016 per page. . Insertions into existing statutes are indicated by underscores. Deletions from existing statutes are indicated by overstrikes."}], "amendments": [], "fiscal_notes": [], "action_log": [{"bill_id": 14660, "action_date": "1999-01-27T14:00:00-06:00", "document_id": 82120, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/82120.pdf", "status_text": "First read in House and referred to", "journal_page": 215, "committee_id_action": 628, "committee_id_assigned": 623, "result": "N", "vote": {}}, {"bill_id": 14660, "action_date": "1999-02-04T19:00:00-06:00", "document_id": 81959, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/81959.pdf", "status_text": "Scheduled for Committee hearing on this date", "journal_page": null, "committee_id_action": 623, "committee_id_assigned": null, "result": "N", "vote": {}}, {"bill_id": 14660, "action_date": "1999-02-04T19:00:00-06:00", "document_id": 81959, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/81959.pdf", "status_text": "Deferred to 41st legislative day", "journal_page": 390, "committee_id_action": 623, "committee_id_assigned": null, "result