tart_seconds": 0.0}, {"meeting_datetime": "2004-02-17T00:00:00-06:00", "committee": "HAG", "url": "https://sdpb.sd.gov/sdpbpodcast/2004/hag25sb0188a.mp3", "start_seconds": 0.0}, {"meeting_datetime": "2004-02-17T00:00:00-06:00", "committee": "HAG", "url": "https://sdpb.sd.gov/sdpbpodcast/2004/hag25sb0188a.mp3", "start_seconds": 0.0}, {"meeting_datetime": "2004-02-19T00:00:00-06:00", "committee": "H", "url": "https://sdpb.sd.gov/sdpbpodcast/2004/hou27sb0188a.mp3", "start_seconds": 0.0}, {"meeting_datetime": "2004-02-19T00:00:00-06:00", "committee": "H", "url": "https://sdpb.sd.gov/sdpbpodcast/2004/hou27sb0188a.mp3", "start_seconds": 0.0}, {"meeting_datetime": "2004-02-19T00:00:00-06:00", "committee": "H", "url": "https://sdpb.sd.gov/sdpbpodcast/2004/hou27sb0188a.mp3", "start_seconds": 0.0}], "bill_versions": [{"bill_id": 19152, "bill_version_id": 99906, "bill_version": "Introduced", "bill_version_date": "2004-01-26T11:56:55-06:00", "bill_text": "FOR AN ACT ENTITLED, An Act to revise certain provisions relating to access to isolated tracts of land. BE IT ENACTED BY THE LEGISLATURE OF THE STATE OF SOUTH DAKOTA: Section 1. That \u00a7 31-22-1 be amended to read as follows: 31-22-1. Every owner of an isolated tract of land containing at least ten acres not touched by a passable public highway or smaller tract of land containing at least five acres used or intended to be used in good faith in whole or in part for residential purposes is entitled to an easement or right-of-way across adjacent lands to reach a public highway, which easement or right-of-way may be secured as provided in this chapter. An isolated tract is further defined as an area which is either inaccessible by motor vehicle because of natural barriers from all other land owned by the owner of the isolated tract or is such an area which is not touched by a passable public highway, which is in use or reasonably usable for motor vehicles as distinguished from public highways existing only by operation of law . A tract of land adjoining a section line right-of-way for at least sixty-six feet is not an isolated tract . 350 copies of this document were printed by the South Dakota Legislative Research Council at a cost of $.027 per page. . Insertions into existing statutes are indicated by underscores. Deletions from existing statutes are indicated by overstrikes."}, {"bill_id": 19152, "bill_version_id": 101431, "bill_version": "House Agriculture and Natural Resources Engrossed", "bill_version_date": "2004-02-17T12:40:29-06:00", "bill_text": "FOR AN ACT ENTITLED, An Act to revise certain provisions relating to access to isolated tracts of land. BE IT ENACTED BY THE LEGISLATURE OF THE STATE OF SOUTH DAKOTA: Section 1. That \u00a7 31-22-1 be amended to read as follows: 31-22-1. Every owner of an isolated tract of land containing at least ten acres not touched by a passable public highway or smaller tract of land containing at least five acres used or intended to be used in good faith in whole or in part for residential purposes is entitled to an easement or right-of-way across adjacent lands to reach a public highway, which easement or right-of-way may be secured as provided in this chapter. An isolated tract is further defined as an area which is either inaccessible by motor vehicle because of natural barriers from all other land owned by the owner of the isolated tract or is such an area which is not touched by a passable public highway, which is in use or reasonably usable for motor vehicles as distinguished from public highways existing only by operation of law . A tract of land adjoining a section line right-of-way for at least sixty-six feet is not an isolated tract if a passable road can be built within the adjoining section line to connect to a passable public highway . 350 copies of this document were printed by the South Dakota Legislative Research Council at a cost of $.027 per page. . Insertions into existing statutes are indicated by underscores. Deletions from existing statutes are indicated by overstrikes."}, {"bill_id": 19152, "bill_version_id": 101791, "bill_version": "Enrolled", "bill_version_date": "2004-02-21T14:31:57-06:00", "bill_text": ""}], "amendments": [], "fiscal_notes": [], "action_log": [{"bill_id": 19152, "action_date": "2004-01-26T14:00:00-06:00", "document_id": 99956, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/99956.pdf", "status_text": "First read in Senate and referred to", "journal_page": 149, "committee_id_action": 832, "committee_id_assigned": 805, "result": "N", "vote": {}}, {"bill_id": 19152, "action_date": "2004-01-29T10:00:00-06:00", "document_id": 100254, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/100254.pdf", "status_text": "Scheduled for Committee hearing on this date", "journal_page": null, "committee_id_action": 805, "committee_id_assigned": null, "result": "N", "vote": {}}, {"bill_id": 19152, "action_date": "2004-02-03T10:00:00-06:00", "document_id": 100515, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/100515.pdf", "status_text": "Scheduled for Committee hearing on this date", "journal_page": null, "committee_id_action": 805, "committee_id_assigned": null, "result": "N", "vote": {}}, {"bill_id": 19152, "action_date": "2004-02-03T10:00:00-06:00", "document_id": 100515, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/100515.pdf", "status_text": "Do Pass,", "journal_page": 286, "committee_id_action": 805, "committee_id_assigned": null, "result": "P", "vote": {"vote_id": 59055, "president_vote": null, "Nay": [3059], "Yea": [3061, 3092, 3097, 3098, 3101, 3139, 3103, 3064]}}, {"bill_id": 19152, "action_date": "2004-02-04T14:00:00-06:00", "document_id": 100652, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/100652.pdf", "status_text": "Do Pass,", "journal_page": 325, "committee_id_action": 832, "committee_id_assigned": null, "result": "P", "vote": {"vote_id": 59198, "president_vote": null, "Yea": [3044, 3046, 3