sions": [{"bill_id": 8328, "bill_version_id": 47270, "bill_version": "Introduced", "bill_version_date": "2018-01-18T11:29:34.047-06:00", "bill_text": "FOR AN ACT ENTITLED, An Act to establish term limits for members of the Board of Education Standards. BE IT ENACTED BY THE LEGISLATURE OF THE STATE OF SOUTH DAKOTA: Section 1. That \u00a7 1-45-6.1 be amended to read: 1-45-6.1. There is created within the Department of Education the South Dakota Board of Education Standards. The South Dakota Board of Education Standards shall consist of seven members. The members shall be appointed by the Governor with the advice and consent of the Senate. The terms of office for each member shall be four years and shall terminate December thirty-first of the fourth year. No member appointed after July 1, 2018, may serve more than three consecutive terms. However, after serving three consecutive terms, a member may be reappointed after at least two years have passed since the expiration of the member's last term. 100 copies were printed on recycled paper by the South Dakota Legislative Research Council at a cost of $.167 per page. . Insertions into existing statutes are indicated by underscores. Deletions from existing statutes are indicated by overstrikes."}, {"bill_id": 8328, "bill_version_id": 48009, "bill_version": "Senate Education Engrossed", "bill_version_date": "2018-01-25T12:57:16.7-06:00", "bill_text": "FOR AN ACT ENTITLED, An Act to establish term limits for members of the Board of Education Standards. BE IT ENACTED BY THE LEGISLATURE OF THE STATE OF SOUTH DAKOTA: Section 1. That \u00a7 1-45-6.1 be amended to read: 1-45-6.1. There is created within the Department of Education the South Dakota Board of Education Standards. The South Dakota Board of Education Standards shall consist of seven members. The members shall be appointed by the Governor with the advice and consent of the Senate. The terms of office for each member shall be four years and shall terminate December thirty-first of the fourth year. No member appointed after July 1, 2018, may serve more than three consecutive terms. However, after serving three consecutive terms, a member may be reappointed after at least two years have passed since the expiration of the member's last term. Any partial term to fill a vacancy on the board may not count against the three-term limit. 100 copies were printed on recycled paper by the South Dakota Legislative Research Council at a cost of $.167 per page. . Insertions into existing statutes are indicated by underscores. Deletions from existing statutes are indicated by overstrikes."}, {"bill_id": 8328, "bill_version_id": 49572, "bill_version": "House Education Engrossed", "bill_version_date": "2018-02-21T12:54:28.07-06:00", "bill_text": "FOR AN ACT ENTITLED, An Act to establish term limits for members of the Board of Education Standards. BE IT ENACTED BY THE LEGISLATURE OF THE STATE OF SOUTH DAKOTA: Section 1. That \u00a7 1-45-6.1 be amended to read: 1-45-6.1. There is created within the Department of Education the South Dakota Board of Education Standards. The South Dakota Board of Education Standards shall consist of seven members. The members shall be appointed by the Governor with the advice and consent of the Senate. The terms of office for each member shall be four years and shall terminate December thirty-first of the fourth year. No member appointed after July 1, 2018, and without any previous service on the board, may serve more than three consecutive terms. However, after serving three consecutive terms, a member may be reappointed after at least two years have passed since the expiration of the member's last term. Any partial term to fill a vacancy on the board may not count against the three-term limit. 100 copies were printed on recycled paper by the South Dakota Legislative Research Council at a cost of $.167 per page. . Insertions into existing statutes are indicated by underscores. Deletions from existing statutes are indicated by overstrikes."}, {"bill_id": 8328, "bill_version_id": 50129, "bill_version": "Enrolled", "bill_version_date": "2018-03-01T09:18:53.153-06:00", "bill_text": ""}], "amendments": [], "fiscal_notes": [], "action_log": [{"bill_id": 8328, "action_date": "2018-01-18T14:00:00-06:00", "document_id": 47299, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/47299.pdf", "status_text": "First read in Senate and referred to", "journal_page": 135, "committee_id_action": 395, "committee_id_assigned": 370, "result": "N", "vote": {}}, {"bill_id": 8328, "action_date": "2018-01-25T07:45:00-06:00", "document_id": 47619, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/47619.pdf", "status_text": "Scheduled for hearing", "journal_page": 0, "committee_id_action": 370, "committee_id_assigned": null, "result": "N", "vote": {}}, {"bill_id": 8328, "action_date": "2018-01-25T07:45:00-06:00", "document_id": 47984, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/47984.pdf", "status_text": "Amend the amendment", "journal_page": 0, "committee_id_action": 370, "committee_id_assigned": null, "result": "P", "vote": {}}, {"bill_id": 8328, "action_date": "2018-01-25T07:45:00-06:00", "document_id": 47984, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/47984.pdf", "status_text": "Amend the amendment", "journal_page": 0, "committee_id_action": 370, "committee_id_assigned": null, "result": "P", "vote": {}}, {"bill_id": 8328, "action_date": "2018-01-25T07:45:00-06:00", "document_id": 47984, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/47984.pdf", "status_text": "Do Pass Amended", "journal_page": 237, "committee_id_action": 370, "committee_id_assigned": null, "result": "P", "vote": {"vote_id": 24923, "president_vote": null, "Yea": [1262, 1234, 1278, 1245, 1271, 1230], "Excused": [1267]}}, {"bill_id": 8328, "action_date": "2018-01-25T07:45:00-06:00", "document_id": 47984, "document_url": "https://mylrc.sdlegislature.gov/api/Docu