seconds": 1303.0}], "bill_versions": [{"bill_id": 3391, "bill_version_id": 19594, "bill_version": "Introduced", "bill_version_date": "2012-01-20T08:46:51.23-06:00", "bill_text": "FOR AN ACT ENTITLED, An Act to retain certain members of municipal governing bodies in office following the redistricting of municipal wards. BE IT ENACTED BY THE LEGISLATURE OF THE STATE OF SOUTH DAKOTA: Section 1. That chapter 9-13 be amended by adding thereto a NEW SECTION to read as follows: Notwithstanding the redistricting provided in \u00a7 9-13-16, the tenure of office of any member of the governing body whose term of office extends beyond the next annual election is not affected, and the governing body, as part of the redistricting process, shall designate the wards to be represented by such members. Such council members may or may not be residents of the district they are designated to represent. Each ward for which representation is not provided by such designation shall, at the next ensuing annual election, elect a council member, the term of office to be determined as provided in \u00a7 9-8-4. 180 copies were printed on recycled paper by the South Dakota Legislative Research Council at a cost of $.063 per page. . Insertions into existing statutes are indicated by underscores. Deletions from existing statutes are indicated by overstrikes."}, {"bill_id": 3391, "bill_version_id": 20304, "bill_version": "House Local Government Engrossed", "bill_version_date": "2012-01-26T13:20:30.557-06:00", "bill_text": "FOR AN ACT ENTITLED, An Act to retain certain members of municipal governing bodies in office following the redistricting of municipal wards. BE IT ENACTED BY THE LEGISLATURE OF THE STATE OF SOUTH DAKOTA: Section 1. That chapter 9-13 be amended by adding thereto a NEW SECTION to read as follows: Notwithstanding the redistricting provided in \u00a7 9-13-16, the term of office of any member of the governing body whose term of office extends beyond the next annual election is not affected, and the governing body, as part of the redistricting process, shall designate the wards to be represented by such members. Such council members may or may not be residents of the district they are designated to represent. Each ward for which representation is not provided by such designation shall, at the next ensuing annual election, elect a council member, the term of office to be determined as provided in \u00a7 9-8-4. 180 copies were printed on recycled paper by the South Dakota Legislative Research Council at a cost of $.063 per page. . Insertions into existing statutes are indicated by underscores. Deletions from existing statutes are indicated by overstrikes."}, {"bill_id": 3391, "bill_version_id": 21165, "bill_version": "Enrolled", "bill_version_date": "2012-02-10T08:21:14.163-06:00", "bill_text": ""}], "amendments": [], "fiscal_notes": [], "action_log": [{"bill_id": 3391, "action_date": "2012-01-20T14:00:00-06:00", "document_id": 19670, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/19670.pdf", "status_text": "First read in House and referred to", "journal_page": 66, "committee_id_action": 173, "committee_id_assigned": 164, "result": "N", "vote": {}}, {"bill_id": 3391, "action_date": "2012-01-26T10:00:00-06:00", "document_id": 20039, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/20039.pdf", "status_text": "Scheduled for hearing", "journal_page": 0, "committee_id_action": 164, "committee_id_assigned": null, "result": "N", "vote": {}}, {"bill_id": 3391, "action_date": "2012-01-26T10:00:00-06:00", "document_id": 20297, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/20297.pdf", "status_text": "Motion to amend", "journal_page": 0, "committee_id_action": 164, "committee_id_assigned": null, "result": "P", "vote": {}}, {"bill_id": 3391, "action_date": "2012-01-26T10:00:00-06:00", "document_id": 20297, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/20297.pdf", "status_text": "Do Pass Amended", "