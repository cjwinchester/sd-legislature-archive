ons"], "audio": [{"meeting_datetime": "2009-02-11T00:00:00-06:00", "committee": "SST", "url": "https://sdpb.sd.gov/sdpbpodcast/2009/sst20.mp3", "start_seconds": 2836.0}, {"meeting_datetime": "2009-02-11T00:00:00-06:00", "committee": "SST", "url": "https://sdpb.sd.gov/sdpbpodcast/2009/sst20.mp3", "start_seconds": 2836.0}, {"meeting_datetime": "2009-02-11T00:00:00-06:00", "committee": "SST", "url": "https://sdpb.sd.gov/sdpbpodcast/2009/sst20.mp3", "start_seconds": 2836.0}, {"meeting_datetime": "2009-02-12T00:00:00-06:00", "committee": "SEN", "url": "https://sdpb.sd.gov/sdpbpodcast/2009/sen21.mp3", "start_seconds": 4164.0}, {"meeting_datetime": "2009-02-12T00:00:00-06:00", "committee": "SEN", "url": "https://sdpb.sd.gov/sdpbpodcast/2009/sen21.mp3", "start_seconds": 4164.0}, {"meeting_datetime": "2009-02-12T00:00:00-06:00", "committee": "SEN", "url": "https://sdpb.sd.gov/sdpbpodcast/2009/sen21.mp3", "start_seconds": 4164.0}], "bill_versions": [{"bill_id": 660, "bill_version_id": 6297, "bill_version": "Introduced", "bill_version_date": "2009-01-29T11:19:23.997-06:00", "bill_text": "A JOINT RESOLUTION, Proposing and submitting to the electors at the next general election an amendment to Article VI of the Constitution of the State of South Dakota, relating to the utilization of English as the official language. BE IT RESOLVED BY THE SENATE OF THE STATE OF SOUTH DAKOTA, THE HOUSE OF REPRESENTATIVES CONCURRING THEREIN: Section 1. That at the next general election held in the state, the following amendment to Article VI of the Constitution of the State of South Dakota, as set forth in section 2 of this Joint Resolution, which is hereby agreed to, shall be submitted to the electors of the state for approval. Section 2. That Article VI of the Constitution of the State of South Dakota, be amended by adding a NEW SECTION to read as follows: \u00a7 28. The English language is the official language of the state of South Dakota. The Legislature may enact no statute, no state agency may promulgate any rule, and no governmental subdivision may pass any ordinance, which requires the use of any language other than English. No provision of this section may be construed to conflict with the Constitution or statutes of the United States. No provision of this section may be construed to restrict the right of any tribal government to utilize or encourage the use of the Lakota, Nakota, and Dakota languages. No provision of this section may be construed to prevent or discourage the voluntary usage by anyone of any language other than English."}], "amendments": [], "fiscal_notes": [], "action_log": [{"bill_id": 660, "action_date": "2009-01-29T14:00:00-06:00", "document_id": 6350, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/6350.pdf", "status_text": "First read in Senate and referred to", "journal_page": 233, "committee_id_action": 54, "committee_id_assigned": 39, "result": "N", "vote": {}}, {"bill_id": 660, "action_date": "2009-02-04T10:00:00-06:00", "document_id": 6606, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/6606.pdf", "status_text": "Scheduled for hearing", "journal_page": 0, "committee_id_action": 39, "committee_id_assigned": null, "result": "N", "vote": {}}, {"bill_id": 660, "action_date": "2009-02-11T10:00:00-06:00", "document_id": 7025, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/7025.pdf", "status_text": "Scheduled for hearing", "journal_page": 0, "committee_id_action": 39, "committee_id_assigned": null, "result": "N", "vote": {}}, {"bill_id": 660, "action_date": "2009-02-11T10:00:00-06:00", "document_id": 7238, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/7238.pdf", "status_text": "Do Pass", "journal_page": 380, "committee_id_action": 39, "committee_id_assigned": null, "result": "P", "vote": {"vote_id": 3917, "president_vote": null, "Yea": [653, 677, 634, 661, 689], "Nay": [659, 648, 642, 649]}}, {"bill_id": 660, "action_date": "2009-02-12T14:00:00-06:00", "document_id