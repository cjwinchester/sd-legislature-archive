atutes are indicated by underscores. Deletions from existing statutes are indicated by overstrikes."}], "amendments": [], "fiscal_notes": [], "action_log": [{"bill_id": 19063, "action_date": "2004-01-22T14:00:00-06:00", "document_id": 99662, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/99662.pdf", "status_text": "First read in Senate and referred to", "journal_page": 111, "committee_id_action": 832, "committee_id_assigned": 806, "result": "N", "vote": {}}, {"bill_id": 19063, "action_date": "2004-01-27T10:00:00-06:00", "document_id": 100041, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/100041.pdf", "status_text": "Scheduled for Committee hearing on this date", "journal_page": null, "committee_id_action": 806, "committee_id_assigned": null, "result": "N", "vote": {}}, {"bill_id": 19063, "action_date": "2004-02-03T10:00:00-06:00", "document_id": 100529, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/100529.pdf", "status_text": "Scheduled for Committee hearing on this date", "journal_page": null, "committee_id_action": 806, "committee_id_assigned": null, "result": "N", "vote": {}}, {"bill_id": 19063, "action_date": "2004-02-03T10:00:00-06:00", "document_id": 100529, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/100529.pdf", "status_text": "Hog Housed", "journal_page": null, "committee_id_action": 806, "committee_id_assigned": null, "result": "P", "vote": {}}, {"bill_id": 19063, "action_date": "2004-02-03T10:00:00-06:00", "document_id": 100529, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/100529.pdf", "status_text": "Deferred to 36th legislative day", "journal_page": 312, "committee_id_action": 806, "committee_id_assigned": null, "result": "P", "vote": {"vote_id": 59088, "president_vote": null, "Nay": [3044, 3089, 3137], "Yea": [3094, 3132, 3050, 3109]}}], "rss_feed": "https://sdlegislature.gov/api/Bills/RSS/19063"}