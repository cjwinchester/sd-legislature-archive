ns": [{"bill_id": 23141, "bill_version_id": 231311, "bill_version": "Introduced", "bill_version_date": "2022-01-31T11:37:53.963-06:00", "bill_text": "An Act to prohibit health insurance policies from restricting the method of payment for dental care. Be it enacted by the Legislature of the State of South Dakota: Section 1. That chapter 58-17 be amended with a NEW SECTION: A health insurance policy may not require payment for dental care by credit card."}, {"bill_id": 23141, "bill_version_id": 232868, "bill_version": "Senate Commerce and Energy Engrossed", "bill_version_date": "2022-02-09T08:30:39.92-06:00", "bill_text": "An Act to prohibit insurance policies from restricting the method of payment for dental care. Be it enacted by the Legislature of the State of South Dakota: Section 1. That chapter 58-17 be amended with a NEW SECTION: No plan of insurance insuring dental care services may contain restrictions on methods of payment to the dentist in which the only acceptable payment method is a credit card payment."}, {"bill_id": 23141, "bill_version_id": 234117, "bill_version": "Enrolled", "bill_version_date": "2022-02-22T10:16:23.44-06:00", "bill_text": "ENTITLED An Act to prohibit insurance policies from restricting the method of payment for dental care. Be it enacted by the Legislature of the State of South Dakota: Section 1. That chapter 58-17 be amended with a NEW SECTION: No plan of insurance insuring dental care services may contain restrictions on methods of payment to the dentist in which the only acceptable payment method is a credit card payment. An Act to prohibit insurance policies from restricting the method of payment for dental care. I certify that the attached Act originated in the: Senate as Bill No. 171 Secretary of the Senate President of the Senate Attest: Secretary of the Senate Speaker of the House Attest: Chief Clerk Senate Bill No. 171 File No. ____ Chapter No. ______ Received at this Executive Office this _____ day of _____________, 2022 at ____________M. By for the Governor The attached Act is hereby approved this ________ day of ______________, A.D., 2022 Governor STATE OF SOUTH DAKOTA, ss. Office of the Secretary of State Filed ____________, 2022 at _________ o'clock __M. Secretary of State By Asst. Secretary of State"}], "amendments": [], "fiscal_notes": [], "action_log": [{"bill_id": 23141, "action_date": "2022-01-31T14:00:00-06:00", "document_id": 230983, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/230983.pdf", "status_text": "First read in Senate and referred to", "journal_page": 139, "committee_id_action": 1034, "committee_id_assigned": 1022, "result": "N", "vote": {}}, {"bill_id": 23141, "action_date": "2022-02-08T10:00:00-06:00", "document_id": 232423, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/232423.pdf", "status_text": "Scheduled for hearing", "journal_page": null, "committee_id_action": 1022, "committee_id_assigned": null, "result": "N", "vote": {}}, {"bill_id": 23141, "action_date": "2022-02-08T10:00:00-06:00", "document_id": 232426, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/232426.pdf", "status_text": "Motion to amend", "journal_page": 1, "committee_id_action": 1022, "committee_id_assigned": null, "result": "P", "vote": {}}, {"bill_id": 23141, "action_date": "2022-02-08T10:00:00-06:00", "document_id": 232426, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/232426.pdf", "status_text": "Do Pass Amended", "journal_page": 1, "committee_id_action": 1022, "committee_id_assigned": null, "result": "P", "vote": {"vote_id": 73043, "president_vote": null, "Yea": [3939, 3975, 3993, 3999, 4000, 4006, 4016, 4010, 3925]}}, {"bill_id": 23141, "action_date": "2022-02-08T10:00:00-06:00", "document_id": 232426, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/232426.pdf", "status_text": "Certified uncontested, placed on consent", "journal_page": 1, "committee_id_action": 1022, "committee_id_assigned": null, "result":