0}], "bill_versions": [{"bill_id": 1896, "bill_version_id": 11306, "bill_version": "Introduced", "bill_version_date": "2010-01-28T10:39:13.833-06:00", "bill_text": "FOR AN ACT ENTITLED, An Act to provide for the temporary curtailment of out-of-state legislative travel. BE IT ENACTED BY THE LEGISLATURE OF THE STATE OF SOUTH DAKOTA: Section 1. No state legislator is eligible to be reimbursed for out-of-state travel incurred in behalf of his or her legislative duties during fiscal year 2011 and fiscal year 2012. However, the Executive Board of the Legislative Research Council may waive the provisions of this Act by a two-thirds vote, if, in the considered opinion of the members of the Executive Board, the presence and participation of an individual state legislator at an out-of-state venue is critical to the best interests of the state of South Dakota. 180 copies of this document were printed by the South Dakota Legislative Research Council at a cost of $.052 per page. . Insertions into existing statutes are indicated by underscores. Deletions from existing statutes are indicated by overstrikes."}], "amendments": [], "fiscal_notes": [], "action_log": [{"bill_id": 1896, "action_date": "2010-01-28T14:00:00-06:00", "document_id": 11385, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/11385.pdf", "status_text": "First read in House and referred to", "journal_page": 226, "committee_id_action": 86, "committee_id_assigned": 79, "result": "N", "vote": {}}, {"bill_id": 1896, "action_date": "2010-02-10T07:45:00-06:00", "document_id": 11853, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/11853.pdf", "status_text": "Scheduled for hearing", "journal_page": 0, "committee_id_action": 79, "committee_id_assigned": null, "result": "N", "vote": {}}, {"bill_id": 1896, "action_date": "2010-02-10T07:45:00-06:00", "document_id": 12022, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/12022.pdf", "status_text": "Deferred to the 41st legislative day