rsions": [{"bill_id": 4239, "bill_version_id": 24223, "bill_version": "Introduced", "bill_version_date": "2013-01-22T10:54:51.72-06:00", "bill_text": "FOR AN ACT ENTITLED, An Act to revise the schedule for payment of excise taxes for farm wineries. BE IT ENACTED BY THE LEGISLATURE OF THE STATE OF SOUTH DAKOTA: Section 1. That \u00a7 35-12-8 be amended to read as follows: 35-12-8. The excise tax on wines established in \u00a7 35-12-7 shall be paid quarterly to the secretary of the Department of Revenue on or before the fifteenth day of the month following the month quarter in which the first sale is made in this state by a licensed farm winery and shall be deposited in the general fund. The farm winery licensee shall file with the secretary a return in the form prescribed by the secretary, and shall keep records and render reports required by the secretary in rules promulgated pursuant to chapter 1-26. 170 copies were printed on recycled paper by the South Dakota Legislative Research Council at a cost of $.093 per page. . Insertions into existing statutes are indicated by underscores. Deletions from existing statutes are indicated by overstrikes."}, {"bill_id": 4239, "bill_version_id": 26005, "bill_version": "Enrolled", "bill_version_date": "2013-02-20T08:29:13.13-06:00", "bill_text": ""}], "amendments": [], "fiscal_notes": [], "action_log": [{"bill_id": 4239, "action_date": "2013-01-22T14:00:00-06:00", "document_id": 24266, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/24266.pdf", "status_text": "First read in House and referred to", "journal_page": 134, "committee_id_action": 202, "committee_id_assigned": 196, "result": "N", "vote": {}}, {"bill_id": 4239, "action_date": "2013-01-29T09:00:00-06:00", "document_id": 24533, "document_url": "https://mylrc.sdlegislature.gov/api/Documents/24533.pdf", "status_text": "Scheduled for hearing", "journal_page": 0, "committee_id_action": 196, "committee_id_assigned": null, "result": "N", "vote": {}}, {"bill_id":